<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

        <meta name="viewport" content="width=device-width,initial-scale=1">

        <title>{% block title %}Altima - test d'entr√©e{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/app/css/style.css') }}" rel="stylesheet" type="text/css"/>
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="container">
            {% include 'header.html.twig' %}

            {% block body %}{% endblock %}

            {% include 'footer.html.twig' %}
         </div>
        {% block javascripts %}
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
            <script>window.jQuery || document.write('<script src="{{ asset('bundles/app/js/jquery-1.6.2.min.js') }}"><\/script>')</script>
            <script defer src="{{ asset('bundles/app/js/jquerytools.min.js') }}"></script>
            <script defer src="{{ asset('bundles/app/js/unobtrusivelib.js') }}"></script>
            <script defer src="{{ asset('bundles/app/js/script.js') }}"></script>
            <script defer src="{{ asset('bundles/app/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
